//Python code for Linear Search
def search(list, num):
    UB = len(list)-1
    LB = 0
    for numbers in list:
        if list[LB] == num:
            return f'Number found at {LB}'
        else:
            LB = LB + 1
    return "Number not found"

list = [1, 2, 3, 4, 5, 6, 7, 8, 9, 20, 40, 223, 1274, 2222]
num = 2222

print(search(list, num))
